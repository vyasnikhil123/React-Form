{
  "name": "React",
  "appName": "dynamic",
  "version": "1.0.0",
  "description": "Completely dynamic front end in React",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env WEBPACK_ENV=build webpack --config webpack.prod.js",
    "watch": "cross-env WEBPACK_ENV=dev webpack --config webpack.dev.js",
    "build-es5": "cross-env WEBPACK_ENV=build webpack --config webpack.prod.es5.js",
    "watch-es5": "cross-env WEBPACK_ENV=dev webpack --config webpack.dev.es5.js"
  },
  "author": "PLabs",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-stars": "^2.2.5",
    "striptags": "^3.1.1"
  }
}
